    "required": [
      "args",
      "data",
      "files",
      "form",
      "headers",
      "json"
    ],
    "additionalProperties": false,
    "properties": {
      "args": {
        "type": "object"
      },
      "data": {
        "type": "string",
        "pattern": "^Test$"
      },
      "files": {
        "type": "object"
      },
      "form": {
        "type": "object"
      },
      "headers": {
        "type": "object"
      },
      "json": {
        "type": "null"
      }
    }
  }
}